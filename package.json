{
  "name": "xpell-ui",
  "version": "2.0.0-alpha.2",
  "description": "Xpell UI JS Framework",
  "files": [
    "dist",
    "types",
    "docs"
  ],
  "types": "./types/index.d.ts",
  "module": "./dist/xpell-ui.es.js",
  "main": "./dist/xpell-ui.cjs.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/xpell-ui.es.js",
      "require": "./dist/xpell-ui.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "docs": "typedoc --options ./typedoc.json --tsconfig ./",
    "publish-alpha-next": "pnpm version prerelease --preid=alpha --no-git-tag-version && pnpm publish --tag alpha"
  },
  "author": "Tamir Fridman <fridman.tamir@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpell-ai/xpell-ui.git"
  },
  "peerDependencies": {
    "xpell-core": "^2.0.0-alpha"
  },
  "dependencies": {
    "animate.css": "^4.1.1"
  },
  "devDependencies": {
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1",
    "xpell-core": "../xpell-core"
  }
}